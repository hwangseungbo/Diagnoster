<?xml version="1.0" encoding="utf-8"?>
<androidx.wear.widget.BoxInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="450px"
    android:layout_height="450px">

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/page4_back" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="101px"
            android:layout_height="18px"
            android:layout_marginBottom="18dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.518"
            app:layout_constraintStart_toStartOf="parent"
            android:src="@drawable/kmcp_nosignal" />

        <ImageView
            android:id="@+id/fr4_iv_page"
            android:layout_width="82px"
            android:layout_height="10px"
            android:layout_marginTop="24dp"
            android:src="@drawable/page_4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fr4_tv_battery"
            android:layout_width="140sp"
            android:layout_height="wrap_content"
            android:layout_marginTop="46dp"
            android:layout_marginEnd="36dp"
            android:fontFamily="@font/helvetica33"
            android:gravity="right"
            android:text="0"
            android:textSize="58sp"
            app:layout_constraintEnd_toEndOf="@+id/fr4_tv_voltdegree"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fr4_tv_voltdegree"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="28dp"
            android:layout_marginBottom="8dp"
            android:fontFamily="@font/helvetica33"
            android:text="  v"
            android:textSize="29sp"
            app:layout_constraintBottom_toBottomOf="@+id/fr4_tv_battery"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/fr4_tv_volttext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/helvetica33"
            android:text="Voltage"
            android:textSize="11sp"
            app:layout_constraintEnd_toEndOf="@+id/fr4_tv_voltdegree"
            app:layout_constraintTop_toBottomOf="@+id/fr4_tv_voltdegree" />

        <ImageView
            android:id="@+id/fr4_iv_volt"
            android:layout_width="20px"
            android:layout_height="33px"
            android:layout_marginStart="11dp"
            android:layout_marginBottom="28dp"
            android:src="@drawable/nosignal_volt"
            app:layout_constraintBottom_toBottomOf="@+id/fr4_tv_voltdegree"
            app:layout_constraintStart_toStartOf="@+id/fr4_tv_voltdegree" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.wear.widget.BoxInsetLayout>